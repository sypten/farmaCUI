{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/ventas/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/?$",
			"dest": "_render"
		},
		{
			"src": "^/buscar/?$",
			"dest": "_render"
		},
		{
			"src": "^/carrito/?$",
			"dest": "_render"
		},
		{
			"src": "^/categoria/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/login/?$",
			"dest": "_render"
		},
		{
			"src": "^/perfil/?$",
			"dest": "_render"
		},
		{
			"src": "^/producto/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/registro/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}